<%= turbo_stream.replace "like-button-for-review-#{@review.id}" do %>
  <%= render 'likes/unlike', review: @review %>
<% end %>

<%= turbo_stream.replace "review-#{@review.id}-likes-count" do %>
  <div id="review-<%= @review.id %>-likes-count" class="text-lightGray text-xl mb-4 ml-2">
    <%= @review.likes.count %>
  </div>
<% end %>
